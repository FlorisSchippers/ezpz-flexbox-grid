{
  "name": "ezpz-grid",
  "version": "1.1.1",
  "description": "A lightweight Sass (.scss) flexbox grid system that's easy to setup and offers a lot of customisation.",
  "main": "lib/_ezpz-flexbox-grid.scss",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "node-sass": "^3.8.0",
    "postcss-cli": "^2.5.2"
  },
  "scripts": {
    "postversion": "git push && git push --tags",
    "sass": "node-sass demo/scss -o demo/css",
    "postsass": "npm run postcss",
    "postcss": "postcss -u autoprefixer --autoprefixer.browsers \"ie 10, last 2 versions\" -r demo/css/*.css",
    "build": "npm run sass",
    "start": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vicompany/ezpz-flexbox-grid.git"
  },
  "keywords": [
    "ezpz",
    "flexbox",
    "grid",
    "vi-company",
    "sass"
  ],
  "author": "VI Company",
  "contributors": [
    "Sven Sigmond (https://github.com/svensigmond)",
    "Sebastiaan van Kerkfort (https://github.com/ba55ie)",
    "Edwin Kalkman (https://github.com/edw1n)",
    "Sjors Spoorendonk (https://github.com/sjorsjes)"
  ],
  "license": "MIT",
  "bugs": "https://github.com/vicompany/ezpz-flexbox-grid/issues",
  "homepage": "https://github.com/vicompany/ezpz-flexbox-grid#readme"
}
